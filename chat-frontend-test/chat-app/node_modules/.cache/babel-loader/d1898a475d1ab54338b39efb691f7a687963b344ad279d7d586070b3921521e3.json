put;\n      this.messages.push({ user: userMessage, bot: '' });\n      this.userInput = '';\n\n      try {\n        const response = await axios.post('http://127.0.0.1:3000/chat/tomaty', {\n          message: userMessage,\n        });\n\n        // ดึงข้อความจาก response\n        const botReply = response.data.response.candidates[0].content.parts[0].text;\n        this.messages[this.messages.length - 1].bot = botReply;\n      } catch (error) {\n        console.error('Error sending message:', error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.chat-container {\n  max-width: 400px;\n  margin: 0 auto;\n  padding: 20px;\n  border: 1px solid #ccc;\n  border-radius: 10px;\n}\n.messages {\n  height: 300px;\n  overflow-y: auto;\n  border: 1px solid #ddd;\n  margin-bottom: 10px;\n  padding: 10px;\n}\n.message {\n  margin-bottom: 10px;\n}\n</style>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACH,SAAS,CAACI,IAAI,CAAC,MAAM,EAAE,EAAE;MAElC,MAAMC,WAAU,GAAI,IAAI,CAACL,SAAS;MAClC,IAAI,CAACC,QAAQ,CAACK,IAAI,CAAC;QAAEC,IAAI,EAAEF,WAAW;QAAEG,GAAG,EAAE;MAAG,CAAC,CAAC;MAClD,IAAI,CAACR,SAAQ,GAAI,EAAE;MAEnB,IAAI;QACF,MAAMS,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAC,mCAAmC,EAAE;UACrEC,OAAO,EAAEN;QACX,CAAC,CAAC;;QAEF;QACA,MAAMO,QAAO,GAAIH,QAAQ,CAACV,IAAI,CAACU,QAAQ,CAACI,UAAU,CAAC,CAAC,CAAC,CAACC,OAAO,CAACC,KAAK,CAAC,CAAC,CAAC,CAACC,IAAI;QAC3E,IAAI,CAACf,QAAQ,CAAC,IAAI,CAACA,QAAQ,CAACgB,MAAK,GAAI,CAAC,CAAC,CAACT,GAAE,GAAII,QAAQ;MACxD,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}