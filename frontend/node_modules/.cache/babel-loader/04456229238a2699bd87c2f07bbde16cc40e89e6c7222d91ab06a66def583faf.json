{"ast":null,"code":"import { TweenMax, TimelineMax, Power1, Power2 } from \"gsap\";\nexport default {\n  data() {\n    return {\n      buttonText: \"REVERSE\",\n      colors: ['#df3891', '#fff78b', '#692286', '#c4a66b', '#ed95c0', '#6ac1b8'],\n      nRombo: 46,\n      timer: 0.8,\n      master: null\n    };\n  },\n  mounted() {\n    this.setObj();\n    this.romboInit();\n    this.initializeAnimations();\n  },\n  methods: {\n    setObj() {\n      const zoomLevel = document.documentElement.clientWidth / window.innerWidth;\n      const heightIOs = window.innerHeight * zoomLevel;\n      if (this.isIOS()) {\n        if (heightIOs > window.innerWidth) {\n          this.$el.style.height = `${heightIOs}px`;\n          this.$el.style.minHeight = `${heightIOs}px`;\n        }\n      }\n      TweenMax.set(this.$el.querySelectorAll('.border-inner')[0], {\n        y: -32\n      });\n      TweenMax.set(this.$el.querySelectorAll('.border-inner')[1], {\n        y: 32\n      });\n      TweenMax.set(this.$el.querySelectorAll('.border-inner')[2], {\n        x: -32\n      });\n      TweenMax.set(this.$el.querySelectorAll('.border-inner')[3], {\n        x: 32\n      });\n    },\n    isIOS() {\n      return /iPhone|iPad|iPod/i.test(navigator.userAgent);\n    },\n    initializeAnimations() {\n      this.master = new TimelineMax({\n        delay: 0.4\n      });\n      this.master.add(this.borderAnimation(), \"scene1\").add(this.romboAnimation(), \"-=1.8\", \"scene2\");\n      this.master.timeScale(this.timer);\n    },\n    borderAnimation() {\n      const tl = new TimelineMax();\n      const borders = this.$el.querySelectorAll('.border-inner');\n      tl.to(borders, 1.8, {\n        x: 0,\n        y: 0,\n        force3D: true,\n        ease: Power1.easeOut,\n        onComplete: () => {\n          document.body.classList.remove('overflow');\n        }\n      });\n      return tl;\n    },\n    romboInit() {\n      const wrapper = this.$refs.wrapper;\n      for (let i = 0; i < this.nRombo; i++) {\n        const gridItem = document.createElement('div');\n        const romboDiv = document.createElement('div');\n        wrapper.appendChild(gridItem);\n        gridItem.className = \"box\";\n        TweenMax.set(\".box\", {\n          perspective: 600,\n          transformOrigin: '50% 50%'\n        });\n        gridItem.appendChild(romboDiv);\n        romboDiv.className = \"rombo\";\n        TweenMax.set(\".rombo\", {\n          transformStyle: \"preserve-3d\"\n        });\n        TweenMax.set(romboDiv, {\n          backgroundColor: this.colors[Math.floor(Math.random() * this.colors.length)],\n          top: this.randomInRange(-40, 40),\n          left: this.randomInRange(-40, 40),\n          y: 0,\n          scale: 0,\n          opacity: 0,\n          transformOrigin: '50% 50%',\n          rotationY: this.randomInRange(-720, 720),\n          rotation: this.randomInRange(-320, 320)\n        });\n      }\n    },\n    randomInRange(min, max) {\n      return Math.floor(Math.random() * (max - min + 1)) + min;\n    },\n    romboAnimation() {\n      const romboTodo = Array.from(this.$el.querySelectorAll('.rombo'));\n      const tl = new TimelineMax();\n      tl.staggerTo(romboTodo, 1.2, {\n        y: 0,\n        scale: 1,\n        opacity: 1,\n        rotationY: 0,\n        rotation: '+=240',\n        force3D: true,\n        ease: Power2.easeOut\n      }, 0.08);\n      return tl;\n    },\n    toggleAnimation() {\n      if (this.master.reversed()) {\n        this.master.play();\n        this.buttonText = \"REVERSE\";\n      } else {\n        this.master.reverse();\n        this.buttonText = \"PLAY\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["TweenMax","TimelineMax","Power1","Power2","data","buttonText","colors","nRombo","timer","master","mounted","setObj","romboInit","initializeAnimations","methods","zoomLevel","document","documentElement","clientWidth","window","innerWidth","heightIOs","innerHeight","isIOS","$el","style","height","minHeight","set","querySelectorAll","y","x","test","navigator","userAgent","delay","add","borderAnimation","romboAnimation","timeScale","tl","borders","to","force3D","ease","easeOut","onComplete","body","classList","remove","wrapper","$refs","i","gridItem","createElement","romboDiv","appendChild","className","perspective","transformOrigin","transformStyle","backgroundColor","Math","floor","random","length","top","randomInRange","left","scale","opacity","rotationY","rotation","min","max","romboTodo","Array","from","staggerTo","toggleAnimation","reversed","play","reverse"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/WelcomeComponent.vue"],"sourcesContent":["<template>\n    <main class=\"Main\" role=\"main\">\n      <a id=\"control\" href=\"#\" @click.prevent=\"toggleAnimation\">{{ buttonText }}</a>\n      <div id=\"borderBlack\" class=\"border\">\n        <div class=\"border-inner\" v-for=\"n in 4\" :key=\"n\"></div>\n      </div>\n      <div id=\"wrapper\" ref=\"wrapper\"></div>\n    </main>\n  </template>\n  \n  <script>\n  import { TweenMax, TimelineMax, Power1, Power2 } from \"gsap\";\n  \n  export default {\n    data() {\n      return {\n        buttonText: \"REVERSE\",\n        colors: ['#df3891', '#fff78b', '#692286', '#c4a66b', '#ed95c0', '#6ac1b8'],\n        nRombo: 46,\n        timer: 0.8,\n        master: null,\n      };\n    },\n    mounted() {\n      this.setObj();\n      this.romboInit();\n      this.initializeAnimations();\n    },\n    methods: {\n      setObj() {\n        const zoomLevel = document.documentElement.clientWidth / window.innerWidth;\n        const heightIOs = window.innerHeight * zoomLevel;\n  \n        if (this.isIOS()) {\n          if (heightIOs > window.innerWidth) {\n            this.$el.style.height = `${heightIOs}px`;\n            this.$el.style.minHeight = `${heightIOs}px`;\n          }\n        }\n  \n        TweenMax.set(this.$el.querySelectorAll('.border-inner')[0], { y: -32 });\n        TweenMax.set(this.$el.querySelectorAll('.border-inner')[1], { y: 32 });\n        TweenMax.set(this.$el.querySelectorAll('.border-inner')[2], { x: -32 });\n        TweenMax.set(this.$el.querySelectorAll('.border-inner')[3], { x: 32 });\n      },\n      isIOS() {\n        return /iPhone|iPad|iPod/i.test(navigator.userAgent);\n      },\n      initializeAnimations() {\n        this.master = new TimelineMax({ delay: 0.4 });\n        this.master.add(this.borderAnimation(), \"scene1\").add(this.romboAnimation(), \"-=1.8\", \"scene2\");\n        this.master.timeScale(this.timer);\n      },\n      borderAnimation() {\n        const tl = new TimelineMax();\n        const borders = this.$el.querySelectorAll('.border-inner');\n  \n        tl.to(borders, 1.8, {\n          x: 0,\n          y: 0,\n          force3D: true,\n          ease: Power1.easeOut,\n          onComplete: () => {\n            document.body.classList.remove('overflow');\n          },\n        });\n  \n        return tl;\n      },\n      romboInit() {\n        const wrapper = this.$refs.wrapper;\n  \n        for (let i = 0; i < this.nRombo; i++) {\n          const gridItem = document.createElement('div');\n          const romboDiv = document.createElement('div');\n  \n          wrapper.appendChild(gridItem);\n          gridItem.className = \"box\";\n  \n          TweenMax.set(\".box\", {\n            perspective: 600,\n            transformOrigin: '50% 50%',\n          });\n  \n          gridItem.appendChild(romboDiv);\n          romboDiv.className = \"rombo\";\n  \n          TweenMax.set(\".rombo\", {\n            transformStyle: \"preserve-3d\",\n          });\n  \n          TweenMax.set(romboDiv, {\n            backgroundColor: this.colors[Math.floor(Math.random() * this.colors.length)],\n            top: this.randomInRange(-40, 40),\n            left: this.randomInRange(-40, 40),\n            y: 0,\n            scale: 0,\n            opacity: 0,\n            transformOrigin: '50% 50%',\n            rotationY: this.randomInRange(-720, 720),\n            rotation: this.randomInRange(-320, 320),\n          });\n        }\n      },\n      randomInRange(min, max) {\n        return Math.floor(Math.random() * (max - min + 1)) + min;\n      },\n      romboAnimation() {\n        const romboTodo = Array.from(this.$el.querySelectorAll('.rombo'));\n        const tl = new TimelineMax();\n  \n        tl.staggerTo(romboTodo, 1.2, {\n          y: 0,\n          scale: 1,\n          opacity: 1,\n          rotationY: 0,\n          rotation: '+=240',\n          force3D: true,\n          ease: Power2.easeOut,\n        }, 0.08);\n  \n        return tl;\n      },\n      toggleAnimation() {\n        if (this.master.reversed()) {\n          this.master.play();\n          this.buttonText = \"REVERSE\";\n        } else {\n          this.master.reverse();\n          this.buttonText = \"PLAY\";\n        }\n      },\n    },\n  };\n  </script>\n  \n  <style lang=\"scss\">\n  $color-white: #fff;\n  $color-pink: #FF3668;\n  $color-border: #000;\n  \n  .Main {\n    display: block;\n    position: relative;\n    width: 100%;\n    height: 100vh;\n    overflow: hidden;\n  \n    .border {\n      position: absolute;\n      top: 0;\n      left: 0;\n      bottom: 0;\n      right: 0;\n      width: 100%;\n      min-height: 100vh;\n      z-index: 10;\n  \n      &-inner {\n        background-color: $color-border;\n        position: absolute;\n        will-change: transform;\n      }\n  \n      &-inner:nth-child(1),\n      &-inner:nth-child(2) {\n        width: 100%;\n        height: 31px;\n      }\n      &-inner:nth-child(3),\n      &-inner:nth-child(4) {\n        width: 31px;\n        height: 100%;\n      }\n      &-inner:nth-child(1) {\n        top: 0;\n        left: 0;\n        transform: translateY(-32px);\n      }\n      &-inner:nth-child(2) {\n        bottom: 0;\n        left: 0;\n        transform: translateY(32px);\n      }\n      &-inner:nth-child(3) {\n        top: 0;\n        left: 0;\n        transform: translateX(-32px);\n      }\n      &-inner:nth-child(4) {\n        top: 0;\n        right: 0;\n        transform: translateX(32px);\n      }\n    }\n  \n    #wrapper {\n      position: absolute;\n      top: 0%;\n      left: 0%;\n      width: 100%;\n      height: 100%;\n      z-index: 0;\n  \n      .box {\n        display: inline-block;\n        float: left;\n        position: relative;\n        width: 16.666%;\n        height: 15vh;\n  \n        .rombo {\n          display: inline-block;\n          background-color: $color-pink;\n          will-change: transform, opacity;\n          opacity: 0;\n          position: absolute;\n          width: 2vw;\n          height: 1vw;\n        }\n      }\n    }\n  \n    @media only screen and (max-width: 736px) {\n      #wrapper {\n        .rombo {\n          width: 4vw;\n          height: 2vw;\n        }\n      }\n  \n      .Main {\n        height: 100vh;\n        min-height: 100vh;\n      }\n  \n      .border {\n        min-height: 100%;\n        &-inner:nth-child(1),\n        &-inner:nth-child(2) {\n          width: 100%;\n          height: 10px;\n        }\n        &-inner:nth-child(3),\n        &-inner:nth-child(4) {\n          width: 10px;\n          height: 100%;\n        }\n      }\n    }\n  \n    #control {\n      background-color: #fff;\n      border: 1px solid #eee;\n      display: block;\n      color: #000;\n      text-decoration: none;\n      font-family: 'Cutive Mono';\n      text-align: center;\n      letter-spacing: 5px;\n      padding: 2px 8px 2px 10px;\n      position: absolute;\n      top: 50%;\n      left: 50%;\n      transform: translate(-50%, -50%);\n      z-index: 999;\n    }\n  }\n  \n  * {\n    margin: 0;\n    padding: 0;\n    -webkit-tap-highlight-color: transparent;\n  }\n  \n  *,\n  *:after,\n  *:before {\n    box-sizing: border-box;\n  }\n  \n  html {\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-rendering: optimizelegibility;\n  }\n  \n  html,\n  body {\n    height: 100%;\n  }\n  \n  body {\n    background-color: $color-white;\n    font-size: 16px;\n  }\n  \n  .overflow {\n    overflow: hidden !important;\n    position: fixed;\n    left: 0;\n    top: 0;\n    height: 100%;\n    width: 100%;\n  }\n  </style>\n  "],"mappings":"AAWE,SAASA,QAAQ,EAAEC,WAAW,EAAEC,MAAM,EAAEC,MAAK,QAAS,MAAM;AAE5D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,SAAS;MACrBC,MAAM,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;MAC1EC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,GAAG;MACVC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,MAAM,CAAC,CAAC;IACb,IAAI,CAACC,SAAS,CAAC,CAAC;IAChB,IAAI,CAACC,oBAAoB,CAAC,CAAC;EAC7B,CAAC;EACDC,OAAO,EAAE;IACPH,MAAMA,CAAA,EAAG;MACP,MAAMI,SAAQ,GAAIC,QAAQ,CAACC,eAAe,CAACC,WAAU,GAAIC,MAAM,CAACC,UAAU;MAC1E,MAAMC,SAAQ,GAAIF,MAAM,CAACG,WAAU,GAAIP,SAAS;MAEhD,IAAI,IAAI,CAACQ,KAAK,CAAC,CAAC,EAAE;QAChB,IAAIF,SAAQ,GAAIF,MAAM,CAACC,UAAU,EAAE;UACjC,IAAI,CAACI,GAAG,CAACC,KAAK,CAACC,MAAK,GAAI,GAAGL,SAAS,IAAI;UACxC,IAAI,CAACG,GAAG,CAACC,KAAK,CAACE,SAAQ,GAAI,GAAGN,SAAS,IAAI;QAC7C;MACF;MAEArB,QAAQ,CAAC4B,GAAG,CAAC,IAAI,CAACJ,GAAG,CAACK,gBAAgB,CAAC,eAAe,CAAC,CAAC,CAAC,CAAC,EAAE;QAAEC,CAAC,EAAE,CAAC;MAAG,CAAC,CAAC;MACvE9B,QAAQ,CAAC4B,GAAG,CAAC,IAAI,CAACJ,GAAG,CAACK,gBAAgB,CAAC,eAAe,CAAC,CAAC,CAAC,CAAC,EAAE;QAAEC,CAAC,EAAE;MAAG,CAAC,CAAC;MACtE9B,QAAQ,CAAC4B,GAAG,CAAC,IAAI,CAACJ,GAAG,CAACK,gBAAgB,CAAC,eAAe,CAAC,CAAC,CAAC,CAAC,EAAE;QAAEE,CAAC,EAAE,CAAC;MAAG,CAAC,CAAC;MACvE/B,QAAQ,CAAC4B,GAAG,CAAC,IAAI,CAACJ,GAAG,CAACK,gBAAgB,CAAC,eAAe,CAAC,CAAC,CAAC,CAAC,EAAE;QAAEE,CAAC,EAAE;MAAG,CAAC,CAAC;IACxE,CAAC;IACDR,KAAKA,CAAA,EAAG;MACN,OAAO,mBAAmB,CAACS,IAAI,CAACC,SAAS,CAACC,SAAS,CAAC;IACtD,CAAC;IACDrB,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAACJ,MAAK,GAAI,IAAIR,WAAW,CAAC;QAAEkC,KAAK,EAAE;MAAI,CAAC,CAAC;MAC7C,IAAI,CAAC1B,MAAM,CAAC2B,GAAG,CAAC,IAAI,CAACC,eAAe,CAAC,CAAC,EAAE,QAAQ,CAAC,CAACD,GAAG,CAAC,IAAI,CAACE,cAAc,CAAC,CAAC,EAAE,OAAO,EAAE,QAAQ,CAAC;MAC/F,IAAI,CAAC7B,MAAM,CAAC8B,SAAS,CAAC,IAAI,CAAC/B,KAAK,CAAC;IACnC,CAAC;IACD6B,eAAeA,CAAA,EAAG;MAChB,MAAM