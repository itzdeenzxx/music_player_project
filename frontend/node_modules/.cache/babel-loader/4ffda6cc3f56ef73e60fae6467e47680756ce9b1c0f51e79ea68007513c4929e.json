iv class=\"progress mb-3\" style=\"height: 8px; cursor: pointer;\" @click=\"seekTo\">\r\n                    <div \r\n                      class=\"progress-bar bg-danger\" \r\n                      role=\"progressbar\" \r\n                      :style=\"{ width: `${(currentTime / duration) * 100}%` }\"\r\n                    ></div>\r\n                  </div>\r\n                  \r\n                  <!-- Control Buttons -->\r\n                  <div class=\"d-flex justify-content-between align-items-center\">\r\n                    <div class=\"btn-group\">\r\n                      <button @click=\"togglePlay\" class=\"btn btn-outline-dark\">\r\n                        <i :class=\"isPlaying ? 'fas fa-pause' : 'fas fa-play'\"></i>\r\n                      </button>\r\n                      <button @click=\"toggleLoop\" class=\"btn btn-outline-dark\" :class=\"{ 'active': isLooping }\">\r\n                        <i class=\"fas fa-redo\"></i>\r\n                      </button>\r\n                    </div>\r\n                    \r\n                    <button @click=\"addToFavorites(selectedMusic)\" class=\"btn btn-outline-primary\">\r\n                      <i class=\"fas fa-heart\"></i> Add to Favorites\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n  \r\n        <!-- Music Grid (Same as before) -->\r\n        <div class=\"col-12\">\r\n          <h2 class=\"mb-4\">{{ title }}</h2>\r\n          \r\n          <div v-if=\"loading\" class=\"text-center py-5\">\r\n            <div class=\"spinner-border text-primary\" role=\"status\">\r\n              <span class=\"visually-hidden\">Loading...</span>\r\n            </div>\r\n          </div>\r\n  \r\n          <div v-else-if=\"error\" class=\"alert alert-warning\" role=\"alert\">\r\n            {{ error }}\r\n          </div>\r\n  \r\n          <div v-else-if=\"musicList.length === 0\" class=