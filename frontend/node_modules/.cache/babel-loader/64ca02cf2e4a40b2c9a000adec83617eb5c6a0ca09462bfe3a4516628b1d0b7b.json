{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'PageMusic',\n  data() {\n    return {\n      musicList: [],\n      currentPlaying: null,\n      loading: true,\n      error: null,\n      searchQuery: null\n    };\n  },\n  async created() {\n    // Check if there's a search query in the route params\n    this.searchQuery = this.$route.params.query;\n    if (this.searchQuery) {\n      await this.searchMusic();\n    } else {\n      await this.fetchAllMusic();\n    }\n  },\n  watch: {\n    // Watch for route changes to handle new searches\n    '$route.params.query': {\n      handler: async function (newQuery) {\n        this.searchQuery = newQuery;\n        if (newQuery) {\n          await this.searchMusic();\n        } else {\n          await this.fetchAllMusic();\n        }\n      }\n    }\n  },\n  methods: {\n    async fetchAllMusic() {\n      this.loading = true;\n      try {\n        const response = await axios.get('http://localhost:3000/musics');\n        this.musicList = response.data;\n      } catch (err) {\n        console.error('Error fetching music:', err);\n        this.error = 'Failed to load music list';\n      } finally {\n        this.loading = false;\n      }\n    },\n    async searchMusic() {\n      this.loading = true;\n      try {\n        const response = await axios.get(`http://localhost:3000/musics/search/${this.searchQuery}`);\n        this.musicList = response.data;\n      } catch (err) {\n        console.error('Error searching music:', err);\n        this.error = 'Failed to search music';\n      } finally {\n        this.loading = false;\n      }\n    },\n    playMusic(music) {\n      this.currentPlaying = music;\n      // Scroll to top to show the player\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    },\n    stopPlaying() {\n      this.currentPlaying = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","musicList","currentPlaying","loading","error","searchQ