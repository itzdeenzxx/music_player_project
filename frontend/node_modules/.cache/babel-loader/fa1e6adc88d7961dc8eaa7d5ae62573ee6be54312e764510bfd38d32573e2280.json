3>\r\n              </div>\r\n              <p class=\"album\">Album: {{ song.ms_album }}</p>\r\n              <div class=\"song-details\">\r\n                <span class=\"song-type\">{{ song.ms_type }}</span>\r\n                <span class=\"duration\">{{ formatDuration(song.ms_duration) }}</span>\r\n              </div>\r\n            </div>\r\n            <button\r\n              @click=\"removeFavorite(song.ms_id)\"\r\n              :disabled=\"removingId === song.ms_id\"\r\n              class=\"remove-button\"\r\n              :class=\"{ 'removing': removingId === song.ms_id }\"\r\n            >\r\n              <span v-if=\"removingId !== song.ms_id\">‚ùå</span>\r\n              <span v-else class=\"removing-tomato\">üçÖ</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'FavoriteSongs',\r\n  data() {\r\n    return {\r\n      favorites: [], // Initialize as empty array\r\n      loading: true,\r\n      error: null,\r\n      removingId: null,\r\n      userId: null\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      await this.getUserId();\r\n      if (this.userId) {\r\n        await this.fetchFavorites();\r\n      }\r\n    } catch (error) {\r\n      console.error('Error in component creation:', error);\r\n      this.error = 'Unable to initialize favorites.';\r\n      this.loading = false;\r\n    }\r\n  },\r\n  methods: {\r\n    formatDuration(seconds) {\r\n      if (!seconds) return '--:--';\r\n      const minutes = Math.floor(seconds / 60);\r\n      const remainingSeconds = seconds % 60;\r\n      return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;\r\n    },\r\n    \r\n    async getUserId() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/users/getss');\r\n        if (response.data && response.data.id) {\r\n          this.userId = response.data.id;\r\n        } else {\r\n          throw new Error('No user ID received');\r\n        }\r\n      } catch (err) {\r\n        console.error('Error fetching user session:', err);\r\n        this.error = 'Failed to load user session.';\r\n        throw err;\r\n      }\r\n    },\r\n\r\n    async fetchFavorites() {\r\n      try {\r\n        this.loading = true;\r\n        this.error = null;\r\n        \r\n        // Get favorites list\r\n        const favsResponse = await axios.get(`http://localhost:3000/favs/getfavs/${this.userId}`);\r\n        \r\n        if (!Array.isArray(favsResponse.data)) {\r\n          throw new Error('Invalid favorites data received');\r\n        }\r\n\r\n        // Get details for each favorite\r\n        const favoritesPromises = favsResponse.data.map(async (fav) => {\r\n          try {\r\n            const musicResponse = await axios.get(`http://localhost:3000/musics/${fav.ms_id}`);\r\n            return {\r\n              ...fav,\r\n              ...musicResponse.data\r\n            };\r\n          } catch (err) {\r\n            console.error(`Error fetching details for music ${fav.ms_id}:`, err);\r\n            return fav;\r\n          }\r\n        });\r\n\r\n        // Wait for all requests to complete\r\n        const favoritesWithDetails = await Promise.all(favoritesPromises);\r\n        \r\n        // Update favorites only if we have valid data\r\n        if (favoritesWithDetails.length > 0) {\r\n          this.favorites = favoritesWithDetails;\r\n        } else {\r\n          this.favorites = []; // Set to empty array if no favorites found\r\n        }\r\n        \r\n      } catch (err) {\r\n        console.error('Error fetching favorites:', err);\r\n        this.error = 'Unable to load your favorite songs.';\r\n        this.favorites = []; // Reset to empty array on error\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    async removeFavorite(musicId) {\r\n      try {\r\n        this.removingId = 