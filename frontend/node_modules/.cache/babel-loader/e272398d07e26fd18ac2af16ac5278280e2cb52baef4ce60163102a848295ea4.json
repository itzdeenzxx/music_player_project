","_hoisted_8","error","_hoisted_9","_hoisted_10","_toDisplayString","favorites","length","_hoisted_11","_hoisted_12","_hoisted_13","onClick","_cache","args","$options","goToMusic","_hoisted_14","_Fragment","_renderList","song","ms_id","_hoisted_15","_hoisted_16","_hoisted_17","ms_name","_hoisted_18","ms_album","_hoisted_19","_hoisted_20","ms_type","_hoisted_21","formatDuration","ms_duration","$event","removeFavorite","disabled","removingId","_hoisted_23","_hoisted_24","_hoisted_22"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/FavoriteSongs.vue"],"sourcesContent":["<template>\r\n  <div class=\"favorites-container\">\r\n    <div class=\"tomato-header\"></div>\r\n    <div class=\"favorites-wrapper\">\r\n      <!-- Header -->\r\n      <div class=\"favorites-header\">\r\n        <div class=\"tomato-title\">\r\n          <span class=\"tomato-emoji\">üçÖ</span>\r\n          <h1>My Tomato Tunes</h1>\r\n          <span class=\"tomato-emoji\">üçÖ</span>\r\n        </div>\r\n        <p>My super sweet music collection!</p>\r\n      </div>\r\n\r\n      <!-- Loading State -->\r\n      <div v-if=\"loading\" class=\"loading-state\">\r\n        <div class=\"loading-tomato\">\r\n          <span class=\"tomato-bounce\">üçÖ</span>\r\n        </div>\r\n        <p>Loading your yummy tunes...</p>\r\n      </div>\r\n\r\n      <!-- Error State -->\r\n      <div v-if=\"error\" class=\"error-message\">\r\n        <span class=\"error-icon\">üòø</span>\r\n        <p>{{ error }}</p>\r\n      </div>\r\n\r\n      <!-- Empty State -->\r\n      <div v-if=\"!loading && favorites.length === 0\" class=\"empty-state\">\r\n        <div class=\"empty-tomato\">\r\n          <span class=\"tomato-emoji big-tomato\">üçÖ</span>\r\n          <span class=\"music-note\">‚ô™</span>\r\n          <span class=\"music-note delayed\">‚ô´</span>\r\n        </div>\r\n        <p>Oopsie! No songs in your garden yet!</p>\r\n        <button @click=\"goToMusic\" class=\"browse-button\">\r\n          Plant New Songs! üå±\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Favorites Grid -->\r\n      <div v-if=\"!loading && favorites.length > 0\" class=\"favorites-grid\">\r\n        <div \r\n          v-for=\"song in favorites\" \r\n          :key=\"song.ms_id\"\r\n          class=\"song-card\"\r\n        >\r\n          <div class=\"tomato-decoration\"></div>\r\n          <div class=\"song-content\">\r\n            <div class=\"song-info\">\r\n              <h3>{{ song.ms_name }}</h3>\r\n              <p class=\"album\">Album: {{ song.ms_album }}</p>\r\n              <div class=\"song-details\">\r\n                <span class=\"song-type\">{{ song.ms_type }}</span>\r\n                <span class=\"duration\">üïê {{ formatDuration(song.ms_duration) }}</span>\r\n              </div>\r\n            </div>\r\n            <button\r\n              @click=\"removeFavorite(song.ms_id)\"\r\n              :disabled=\"removingId === song.ms_id\"\r\n              class=\"remove-button\"\r\n            >\r\n              <span v-if=\"removingId !== song.ms_id\">üóëÔ∏è</span>\r\n              <span v-else class=\"removing\">üçÖ</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'FavoriteSongs',\r\n  data() {\r\n    return {\r\n      favorites: [], // Initialize as empty array\r\n      loading: true,\r\n      error: null,\r\n      removingId: null,\r\n      userId: null\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      await this.getUserId();\r\n      if (this.userId) {\r\n        await this.fetchFavorites();\r\n      }\r\n    } catch (error) {\r\n      console.error('Error in component creation:', error);\r\n      this.error = 'Unable to initialize favorites.';\r\n      this.loading = false;\r\n    }\r\n  },\r\n  methods: {\r\n    formatDuration(seconds) {\r\n      if (!seconds) return '--:--';\r\n      const minutes = Math.floor(seconds / 60);\r\n      const remainingSeconds = seconds % 60;\r\n      return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;\r\n    },\r\n    \r\n    async getUserId() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/users/getss');\r\n        if (response.data && response.data.id) {\r\n          this.userId = response.data.id;\r\n        } else {\r\n          throw new Error('No user ID received');\r\n        }\r\n      } catch (err) {\r\n        console.error('Error fetching user session:', err);\r\n        this.error = 'Failed to load user session.';\r\n        throw err;\r\n      }\r\n    },\r\n\r\n    async fetchFavorites() {\r\n      try {\r\n        this.loading = true;\r\n        this.error = null;\r\n        \r\n        // Get favorites list\r\n        const favsResponse = await axios.get(`http://localhost:3000/favs/getfavs/${this.userId}`);\r\n        \r\n        if (!Array.isArray(favsResponse.data)) {\r\n          throw new Error('Invalid favorites data received');\r\n        }\r\n\r\n        // Get details for each favorite\r\n        const favoritesPromises = favsResponse.data.map(async (fav) => {\r\n          try {\r\n            const musicResponse = await axios.get(`http://localhost:3000/musics/${fav.ms_id}`);\r\n            return {\r\n              ...fav,\r\n              ...musicResponse.data\r\n            };\r\n          } catch (err) {\r\n            console.error(`Error fetching details for music ${fav.ms_id}:`, err);\r\n            return fav;\r\n          }\r\n        });\r\n\r\n        // Wait for all requests to complete\r\n        const favoritesWithDetails = await Promise.all(favoritesPromises);\r\n        \r\n        // Update favorites only if we have valid data\r\n        if (favoritesWithDetails.length > 0) {\r\n          this.favorites = favoritesWithDetails;\r\n        } else {\r\n          this.favorites = []; // Set to empty array if no favorites found\r\n        }\r\n        \r\n      } catch (err) {\r\n        console.error('Error fetching favorites:', err);\r\n        this.error = 'Unable to load your favorite songs.';\r\n        this.favorites = []; // Reset to empty array on error\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    async removeFavorite(musicId) {\r\n      try {\r\n        this.removingId = musicId;\r\n        await axios.delete(`http://localhost:3000/favs/deletefav`, {\r\n          data: { \r\n            userId: this.userId,\r\n            musicId: musicId\r\n          }\r\n        });\r\n        this.favorites = this.favorites.filter(song => song.ms_id !== musicId);\r\n      } catch (err) {\r\n        console.error('Error removing favorite:', err);\r\n        this.error = 'Failed to remove song from favorites.';\r\n      } finally {\r\n        this.removingId = null;\r\n      }\r\n    },\r\n\r\n    goToMusic() {\r\n      this.$router.push('/');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.favorites-container {\r\n  min-height: 100vh;\r\n  background-color: #fff5f5;\r\n  padding: 2rem;\r\n  position: relative;\r\n}\r\n\r\n.tomato-header {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  height: 12px;\r\n  background: repeating-linear-gradient(\r\n    90deg,\r\n    #ff6b6b,\r\n    #ff6b6b 20px,\r\n    #ff8787 20px,\r\n    #ff8787 40px\r\n  );\r\n}\r\n\r\n.favorites-wrapper {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.favorites-header {\r\n  margin-bottom: 2rem;\r\n  text-align: center;\r\n}\r\n\r\n.tomato-title {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 1rem;\r\n}\r\n\r\n.tomato-emoji {\r\n  font-size: 2rem;\r\n  animation: wiggle 2s ease-in-out infinite;\r\n}\r\n\r\n.favorites-header h1 {\r\n  font-size: 2.5rem;\r\n  color: #e03131;\r\n  margin-bottom: 0.5rem;\r\n  font-family: \"Comic Sans MS\", cursive;\r\n}\r\n\r\n.favorites-header p {\r\n  color: #ff8787;\r\n  font-size: 1.2rem;\r\n  font-family: \"Comic Sans MS\", cursive;\r\n}\r\n\r\n.loading-state {\r\n  text-align: center;\r\n  padding: 2rem;\r\n  background: white;\r\n  border-radius: 20px;\r\n  box-shadow: 0 4px 12px rgba(255, 107, 107, 0.1);\r\n}\r\n\r\n.loading-tomato {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.tomato-bounce {\r\n  font-size: 3rem;\r\n  display: inline-block;\r\n  animation: bounce 1s infinite;\r\n}\r\n\r\n.empty-state {\r\n  text-align: center;\r\n  padding: 3rem;\r\n  background: white;\r\n  border-radius: 20px;\r\n  box-shadow: 0 4px 12px rgba(255, 107, 107, 0.1);\r\n}\r\n\r\n.empty-tomato {\r\n  position: relative;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.big-tomato {\r\n  font-size: 4rem;\r\n}\r\n\r\n.music-note {\r\n  position: absolute;\r\n  font-size: 1.5rem;\r\n  color: #ff6b6b;\r\n  animation: float 3s ease-in-out infinite;\r\n}\r\n\r\n.music-note:first-of-type {\r\n  top: -10px;\r\n  right: 45%;\r\n}\r\n\r\n.music-note.delayed {\r\n  top: -20px;\r\n  right: 40%;\r\n  animation-delay: 0.5s;\r\n}\r\n\r\n.browse-button {\r\n  background-color: #ff6b6b;\r\n  color: white;\r\n  border: none;\r\n  padding: 0.75rem 1.5rem;\r\n  border-radius: 999px;\r\n  cursor: pointer;\r\n  font-size: 1.1rem;\r\n  font-family: \"Comic Sans MS\", cursive;\r\n  transition: transform 0.2s, background-color 0.2s;\r\n  box-shadow: 0 4px 0 #e03131;\r\n}\r\n\r\n.browse-button:hover {\r\n  background-color: #ff8787;\r\n  transform: translateY(-2px);\r\n}\r\n\r\n.browse-button:active {\r\n  transform: translateY(2px);\r\n  box-shadow: 0 2px 0 #e03131;\r\n}\r\n\r\n.favorites-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\r\n  gap: 1.5rem;\r\n}\r\n\r\n.song-card {\r\n  background: white;\r\n  border-radius: 20px;\r\n  box-shadow: 0 4px 12px rgba(255, 107, 107, 0.1);\r\n  transition: transform 0.2s;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.tomato-decoration {\r\n  position: absolute;\r\n  top: -30px;\r\n  right: -30px;\r\n  width: 60px;\r\n  height: 60px;\r\n  background: #ff8787;\r\n  transform: rotate(45deg);\r\n}\r\n\r\n.song-card:hover {\r\n  transform: translateY(-4px);\r\n}\r\n\r\n.song-content {\r\n  padding: 1.5rem;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: flex-start;\r\n}\r\n\r\n.song-info h3 {\r\n  font-size: 1.25rem;\r\n  color: #e03131;\r\n  margin: 0 0 0.5rem 0;\r\n  font-family: \"Comic Sans MS\", cursive;\r\n}\r\n\r\n.album {\r\n  color: #ff8787;\r\n  font-size: 0.9rem;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.song-type {\r\n  background-color: #fff5f5;\r\n  color: #ff6b6b;\r\n  padding: 0.25rem 0.75rem;\r\n  border-radius: 999px;\r\n  font-size: 0.8rem;\r\n  border: 2px dashed #ff8787;\r\n}\r\n\r\n.remove-button {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  padding: 0.5rem;\r\n  border-radius: 50%;\r\n  transition: all 0.2s;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.remove-button:hover {\r\n  background-color: #fff5f5;\r\n  transform: scale(1.1);\r\n}\r\n\r\n.removing {\r\n  display: inline-block;\r\n  animation: spin 1s linear infinite;\r\n}\r\n\r\n@keyframes wiggle {\r\n  0%, 100% { transform: rotate(0deg); }\r\n  25% { transform: rotate(-10deg); }\r\n  75% { transform: rotate(10deg); }\r\n}\r\n\r\n@keyframes bounce {\r\n  0%, 100% { transform: translateY(0); }\r\n  50% { transform: translateY(-20px); }\r\n}\r\n\r\n@keyframes float {\r\n  0%, 100% { transform: translateY(0) rotate(0deg); }\r\n  50% { transform: translateY(-20px) rotate(10deg); }\r\n}\r\n\r\n@keyframes spin {\r\n  from { transform: rotate(0deg); }\r\n  to { transform: rotate(360deg); }\r\n}\r\n\r\n.error-message {\r\n  background-color: #fff5f5;\r\n  border: 2px dashed #ff6b6b;\r\n  padding: 1rem;\r\n  margin-bottom: 1.5rem;\r\n  border-radius: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .favorites-container {\r\n    padding: 1rem;\r\n  }\r\n\r\n  .favorites-header h1 {\r\n