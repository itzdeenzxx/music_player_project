{"ast":null,"code":"export default {\n  name: 'MusicPlayer',\n  data() {\n    return {\n      musics: [],\n      currentPlayingIndex: -1,\n      isPlaying: false,\n      currentTime: 0,\n      duration: 0,\n      volume: 80,\n      isFavorite: false,\n      youtubePlayer: null,\n      youtubePlayerId: 'youtube-player',\n      playerReady: false,\n      updateInterval: null\n    };\n  },\n  computed: {\n    currentMusic() {\n      return this.currentPlayingIndex >= 0 ? this.musics[this.currentPlayingIndex] : null;\n    },\n    progress() {\n      return this.currentTime / this.duration * 100 || 0;\n    }\n  },\n  methods: {\n    async fetchMusics() {\n      try {\n        const response = await fetch('http://localhost:3000/musics');\n        if (!response.ok) throw new Error('Failed to fetch music data');\n        this.musics = await response.json();\n      } catch (error) {\n        console.error('Error fetching music:', error);\n      }\n    },\n    initYouTubePlayer() {\n      // Load YouTube API\n      if (!window.YT) {\n        const tag = document.createElement('script');\n        tag.src = 'https://www.youtube.com/iframe_api';\n        const firstScriptTag = document.getElementsByTagName('script')[0];\n        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);\n        window.onYouTubeIframeAPIReady = () => {\n          this.createYouTubePlayer();\n        };\n      } else {\n        this.createYouTubePlayer();\n      }\n    },\n    createYouTubePlayer() {\n      this.youtubePlayer = new window.YT.Player(this.youtubePlayerId, {\n        height: '0',\n        width: '0',\n        playerVars: {\n          autoplay: 0,\n          controls: 0,\n          disablekb: 1,\n          enablejsapi: 1,\n          modestbranding: 1,\n          playsinline: 1,\n          rel: 0\n        },\n        events: {\n          onReady: this.onPlayerReady,\n          onStateChange: this.onPlayerStateChange\n        }\n      });\n    },\n    onPlayerReady() {\n      this.playerReady = true;\n      this.youtubePlayer.setVolume(this.volume);\n    },\n    onPlayerStateChange(event) {\n      switch (event.data) {\n        case window.YT.PlayerState.PLAYING:\n          this.isPlaying = true;\n          this.startTimeUpdate();\n          break;\n        case window.YT.PlayerState.PAUSED:\n          this.isPlaying = false;\n          this.stopTimeUpdate();\n          break;\n        case window.YT.PlayerState.ENDED:\n          this.isPlaying = false;\n          this.stopTimeUpdate();\n          this.nextTrack();\n          break;\n      }\n    },\n    startTimeUpdate() {\n      this.updateInterval = setInterval(() => {\n        if (this.youtubePlayer && this.isPlaying) {\n          this.currentTime = this.youtubePlayer.getCurrentTime();\n          this.duration = this.youtubePlayer.getDuration();\n        }\n      }, 1000);\n    },\n    stopTimeUpdate() {\n      if (this.updateInterval) {\n        clearInterval(this.updateInterval);\n      }\n    },\n    extractYouTubeId(url) {\n      const regExp = /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/;\n      const match = url.match(regExp);\n      return match && match[7].length === 11 ? match[7] : false;\n    },\n    async playMusic(index) {\n      if (this.currentPlayingIndex === index) {\n        this.togglePlay();\n        return;\n      }\n      this.currentPlayingIndex = index;\n      const music = this.musics[index];\n      const videoId = this.extractYouTubeId(music.ms_path);\n      if (videoId && this.playerReady) {\n        this.youtubePlayer.loadVideoById(videoId);\n        this.isPlaying = true;\n      }\n    },\n    togglePlay() {\n      if (!this.playerReady) return;\n      if (this.isPlaying) {\n        this.youtubePlayer.pauseVideo();\n      } else {\n        this.youtubePlayer.playVideo();\n      }\n    },\n    nextTrack() {\n      if (this.currentPlayingIndex < this.musics.length - 1) {\n        this.playMusic(this.currentPlayingIndex + 1);\n      }\n    },\n    previousTrack() {\n      if (this.currentPlayingIndex > 0) {\n        this.playMusic(this.currentPlayingIndex - 1);\n      }\n    },\n    formatTime(seconds) {\n      const minutes = Math.floor(seconds / 60);\n      const remainingSeconds = Math.floor(seconds % 60);\n      return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;\n    },\n    seekTime(event) {\n      if (!this.playerReady) return;\n      const rect = event.target.getBoundingClientRect();\n      const x = event.clientX - rect.left;\n      const percentage = x / rect.width;\n      const seekTime = percentage * this.duration;\n      this.youtubePlayer.seekTo(seekTime, true);\n    },\n    adjustVolume(event) {\n      const rect = event.target.getBoundingClientRect();\n      const x = event.clientX - rect.left;\n      const percentage = x / rect.width * 100;\n      this.volume = Math.min(Math.max(percentage, 0), 100);\n      this.$refs.audioPlayer.volume = this.volume / 100;\n    },\n    toggleVolume() {\n      if (this.volume > 0) {\n        this.previousVolume = this.volume;\n        this.volume = 0;\n      } else {\n        this.volume = this.previousVolume || 80;\n      }\n      this.$refs.audioPlayer.volume = this.volume / 100;\n    },\n    toggleFavorite() {\n      this.isFavorite = !this.isFavorite;\n    }\n  },\n  mounted() {\n    this.fetchMusics();\n    this.$refs.audioPlayer.volume = this.volume / 100;\n  }\n};","map":{"version":3,"names":["name","data","musics","currentPlayingIndex","isPlaying","currentTime","duration","volume","isFavorite","youtubePlayer","youtubePlayerId","playerReady","updateInterval","computed","currentMusic","progress","methods","fetchMusics","response","fetch","ok","Error","json","error","console","initYouTubePlayer","window","YT","tag","document","createElement","src","firstScriptTag","getElementsByTagName","parentNode","insertBefore","onYouTubeIframeAPIReady","createYouTubePlayer","Player","height","width","playerVars","autoplay","controls","disablekb","enablejsapi","modestbranding","playsinline","rel","events","onReady","onPlayerReady","onStateChange","onPlayerStateChange","setVolume","event","PlayerState","PLAYING","startTimeUpdate","PAUSED","stopTimeUpdate","ENDED","nextTrack","setInterval","getCurrentTime","getDuration","clearInterval","extractYouTubeId","url","regExp","match","length","playMusic","index","togglePlay","music","videoId","ms_path","loadVideoById","pauseVideo","playVideo","previousTrack","formatTime","seconds","minutes","Math","floor","remainingSeconds","toString","padStart","seekTime","rect","target","getBoundingClientRect","x","clientX","left","percentage","seekTo","adjustVolume","min","max","$refs","audioPlayer","toggleVolume","previousVolume","toggleFavorite","mounted"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/TheHome.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"main\">\r\n      <div class=\"currentplaying\">\r\n        <svg height=\"50px\" width=\"50px\" viewBox=\"0 0 64 64\" xmlns=\"http://www.w3.org/2000/svg\" class=\"spotify\">\r\n          <!-- Existing SVG code remains the same -->\r\n        </svg>\r\n        <p class=\"heading\">Currently Playing</p>\r\n      </div>\r\n      <div v-for=\"(music, index) in musics\" :key=\"music.ms_id\" \r\n           class=\"loader\" \r\n           @click=\"playMusic(index)\">\r\n        <div class=\"song\">\r\n          <p class=\"name\">{{ music.ms_name }}</p>\r\n          <p class=\"artist\">{{ music.ms_album }}</p>\r\n        </div>\r\n        <div class=\"albumcover\"></div>\r\n        <div v-if=\"currentPlayingIndex === index\" class=\"loading\">\r\n          <div class=\"load\"></div>\r\n          <div class=\"load\"></div>\r\n          <div class=\"load\"></div>\r\n          <div class=\"load\"></div>\r\n        </div>\r\n        <div v-else class=\"play\"></div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"card\" v-if=\"currentMusic\">\r\n      <div class=\"top\">\r\n        <div class=\"pfp\">\r\n          <div class=\"playing\">\r\n            <div class=\"greenline line-1\"></div>\r\n            <div class=\"greenline line-2\"></div>\r\n            <div class=\"greenline line-3\"></div>\r\n            <div class=\"greenline line-4\"></div>\r\n            <div class=\"greenline line-5\"></div>\r\n          </div>\r\n        </div>\r\n        <div class=\"texts\">\r\n          <p class=\"title-1\">{{ currentMusic.ms_name }}</p>\r\n          <p class=\"title-2\">{{ currentMusic.ms_album }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- YouTube Player Container -->\r\n      <div class=\"youtube-container\" style=\"display: none;\">\r\n        <div :id=\"youtubePlayerId\"></div>\r\n      </div>\r\n\r\n      <div class=\"controls\">\r\n        <svg @click=\"toggleVolume\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\" height=\"20\" width=\"24\"\r\n          class=\"volume_button\">\r\n          <path clip-rule=\"evenodd\"\r\n            d=\"M11.26 3.691A1.2 1.2 0 0 1 12 4.8v14.4a1.199 1.199 0 0 1-2.048.848L5.503 15.6H2.4a1.2 1.2 0 0 1-1.2-1.2V9.6a1.2 1.2 0 0 1 1.2-1.2h3.103l4.449-4.448a1.2 1.2 0 0 1 1.308-.26Zm6.328-.176a1.2 1.2 0 0 1 1.697 0A11.967 11.967 0 0 1 22.8 12a11.966 11.966 0 0 1-3.515 8.485 1.2 1.2 0 0 1-1.697-1.697A9.563 9.563 0 0 0 20.4 12a9.565 9.565 0 0 0-2.812-6.788 1.2 1.2 0 0 1 0-1.697Zm-3.394 3.393a1.2 1.2 0 0 1 1.698 0A7.178 7.178 0 0 1 18 12a7.18 7.18 0 0 1-2.108 5.092 1.2 1.2 0 1 1-1.698-1.698A4.782 4.782 0 0 0 15.6 12a4.78 4.78 0 0 0-1.406-3.394 1.2 1.2 0 0 1 0-1.698Z\"\r\n            fill-rule=\"evenodd\"></path>\r\n        </svg>\r\n        <div class=\"volume\">\r\n          <div class=\"slider\" @click=\"adjustVolume\">\r\n            <div class=\"green\" :style=\"{ width: volume + '%' }\"></div>\r\n          </div>\r\n          <div class=\"circle\" :style=\"{ right: (100 - volume) + '%' }\"></div>\r\n        </div>\r\n        <svg @click=\"previousTrack\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\" height=\"24\" width=\"24\">\r\n          <path clip-rule=\"evenodd\"\r\n            d=\"M12 21.6a9.6 9.6 0 1 0 0-19.2 9.6 9.6 0 0 0 0 19.2Zm.848-12.352a1.2 1.2 0 0 0-1.696-1.696l-3.6 3.6a1.2 1.2 0 0 0 0 1.696l3.6 3.6a1.2 1.2 0 0 0 1.696-1.696L11.297 13.2H15.6a1.2 1.2 0 1 0 0-2.4h-4.303l1.551-1.552Z\"\r\n            fill-rule=\"evenodd\"></path>\r\n        </svg>\r\n        <svg @click=\"togglePlay\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\" height=\"24\" width=\"24\">\r\n          <path clip-rule=\"evenodd\"\r\n            :d=\"isPlaying \r\n              ? 'M21.6 12a9.6 9.6 0 1 1-19.2 0 9.6 9.6 0 0 1 19.2 0ZM8.4 9.6a1.2 1.2 0 1 1 2.4 0v4.8a1.2 1.2 0 1 1-2.4 0V9.6Zm6-1.2a1.2 1.2 0 0 0-1.2 1.2v4.8a1.2 1.2 0 1 0 2.4 0V9.6a1.2 1.2 0 0 0-1.2-1.2Z'\r\n              : 'M12 21.6a9.6 9.6 0 1 0 0-19.2 9.6 9.6 0 0 0 0 19.2Zm4.448-10.448-3.6-3.6a1.2 1.2 0 0 0-1.696 1.696l1.551 1.552H8.4a1.2 1.2 0 1 0 0 2.4h4.303l-1.551 1.552a1.2 1.2 0 1 0 1.696 1.696l3.6-3.6a1.2 1.2 0 0 0 0-1.696Z'\"\r\n            fill-rule=\"evenodd\"></path>\r\n        </svg>\r\n        <svg @click=\"nextTrack\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\" height=\"24\" width=\"24\">\r\n          <path clip-rule=\"evenodd\"\r\n            d=\"M12 21.6a9.6 9.6 0 1 0 0-19.2 9.6 9.6 0 0 0 0 19.2Zm4.448-10.448-3.6-3.6a1.2 1.2 0 0 0-1.696 1.696l1.551 1.552H8.4a1.2 1.2 0 1 0 0 2.4h4.303l-1.551 1.552a1.2 1.2 0 1 0 1.696 1.696l3.6-3.6a1.2 1.2 0 0 0 0-1.696Z\"\r\n            fill-rule=\"evenodd\"></path>\r\n        </svg>\r\n        <div class=\"air\"></div>\r\n        <svg @click=\"toggleFavorite\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" stroke-width=\"2\" stroke-linejoin=\"round\"\r\n          stroke-linecap=\"round\" class=\"heart\" :style=\"{ fill: isFavorite ? 'pink' : 'none' }\" stroke=\"currentColor\" height=\"20\" width=\"24\">\r\n          <path\r\n            d=\"M3.343 7.778a4.5 4.5 0 0 1 7.339-1.46L12 7.636l1.318-1.318a4.5 4.5 0 1 1 6.364 6.364L12 20.364l-7.682-7.682a4.501 4.501 0 0 1-.975-4.904Z\">\r\n          </path>\r\n        </svg>\r\n      </div>\r\n      \r\n      <div class=\"song-time\">\r\n        <p class=\"timetext time_now\">{{ formatTime(currentTime) }}</p>\r\n        <div class=\"time\" @click=\"seekTime\">\r\n          <div class=\"elapsed\" :style=\"{ width: progress + '%' }\"></div>\r\n        </div>\r\n        <p class=\"timetext time_full\">{{ formatTime(duration) }}</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'MusicPlayer',\r\n  data() {\r\n    return {\r\n      musics: [],\r\n      currentPlayingIndex: -1,\r\n      isPlaying: false,\r\n      currentTime: 0,\r\n      duration: 0,\r\n      volume: 80,\r\n      isFavorite: false,\r\n      youtubePlayer: null,\r\n      youtubePlayerId: 'youtube-player',\r\n      playerReady: false,\r\n      updateInterval: null\r\n    }\r\n  },\r\n  computed: {\r\n    currentMusic() {\r\n      return this.currentPlayingIndex >= 0 ? this.musics[this.currentPlayingIndex] : null;\r\n    },\r\n    progress() {\r\n      return (this.currentTime / this.duration) * 100 || 0;\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchMusics() {\r\n      try {\r\n        const response = await fetch('http://localhost:3000/musics');\r\n        if (!response.ok) throw new Error('Failed to fetch music data');\r\n        this.musics = await response.json();\r\n      } catch (error) {\r\n        console.error('Error fetching music:', error);\r\n      }\r\n    },\r\n    initYouTubePlayer() {\r\n      // Load YouTube API\r\n      if (!window.YT) {\r\n        const tag = document.createElement('script');\r\n        tag.src = 'https://www.youtube.com/iframe_api';\r\n        const firstScriptTag = document.getElementsByTagName('script')[0];\r\n        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);\r\n\r\n        window.onYouTubeIframeAPIReady = () => {\r\n          this.createYouTubePlayer();\r\n        };\r\n      } else {\r\n        this.createYouTubePlayer();\r\n      }\r\n    },\r\n    createYouTubePlayer() {\r\n      this.youtubePlayer = new window.YT.Player(this.youtubePlayerId, {\r\n        height: '0',\r\n        width: '0',\r\n        playerVars: {\r\n          autoplay: 0,\r\n          controls: 0,\r\n          disablekb: 1,\r\n          enablejsapi: 1,\r\n          modestbranding: 1,\r\n          playsinline: 1,\r\n          rel: 0\r\n        },\r\n        events: {\r\n          onReady: this.onPlayerReady,\r\n          onStateChange: this.onPlayerStateChange\r\n        }\r\n      });\r\n    },\r\n    onPlayerReady() {\r\n      this.playerReady = true;\r\n      this.youtubePlayer.setVolume(this.volume);\r\n    },\r\n    onPlayerStateChange(event) {\r\n      switch(event.data) {\r\n        case window.YT.PlayerState.PLAYING:\r\n          this.isPlaying = true;\r\n          this.startTimeUpdate();\r\n          break;\r\n        case window.YT.PlayerState.PAUSED:\r\n          this.isPlaying = false;\r\n          this.stopTimeUpdate();\r\n          break;\r\n        case window.YT.PlayerState.ENDED:\r\n          this.isPlaying = false;\r\n          this.stopTimeUpdate();\r\n          this.nextTrack();\r\n          break;\r\n      }\r\n    },\r\n    startTimeUpdate() {\r\n      this.updateInterval = setInterval(() => {\r\n        if (this.youtubePlayer && this.isPlaying) {\r\n          this.currentTime = this.youtubePlayer.getCurrentTime();\r\n          this.duration = this.youtubePlayer.getDuration();\r\n        }\r\n      }, 1000);\r\n    },\r\n    stopTimeUpdate() {\r\n      if (this.updateInterval) {\r\n        clearInterval(this.updateInterval);\r\n      }\r\n    },\r\n    extractYouTubeId(url) {\r\n      const regExp = /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/;\r\n      const match = url.match(regExp);\r\n      return (match && match[7].length === 11) ? match[7] : false;\r\n    },\r\n    async playMusic(index) {\r\n      if (this.currentPlayingIndex === index) {\r\n        this.togglePlay();\r\n        return;\r\n      }\r\n\r\n      this.currentPlayingIndex = index;\r\n      const music = this.musics[index];\r\n      const videoId = this.extractYouTubeId(music.ms_path);\r\n      \r\n      if (videoId && this.playerReady) {\r\n        this.youtubePlayer.loadVideoById(videoId);\r\n        this.isPlaying = true;\r\n      }\r\n    },\r\n    togglePlay() {\r\n      if (!this.playerReady) return;\r\n      \r\n      if (this.isPlaying) {\r\n        this.youtubePlayer.pauseVideo();\r\n      } else {\r\n        this.youtubePlayer.playVideo();\r\n      }\r\n    },\r\n    nextTrack() {\r\n      if (this.currentPlayingIndex < this.musics.length - 1) {\r\n        this.playMusic(this.currentPlayingIndex + 1);\r\n      }\r\n    },\r\n    previousTrack() {\r\n      if (this.currentPlayingIndex > 0) {\r\n        this.playMusic(this.currentPlayingIndex - 1);\r\n      }\r\n    },\r\n    formatTime(seconds) {\r\n      const minutes = Math.floor(seconds / 60);\r\n      const remainingSeconds = Math.floor(seconds % 60);\r\n      return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;\r\n    },\r\n    seekTime(event) {\r\n      if (!this.playerReady) return;\r\n      \r\n      const rect = event.target.getBoundingClientRect();\r\n      const x = event.clientX - rect.left;\r\n      const percentage = x / rect.width;\r\n      const seekTime = percentage * this.duration;\r\n      this.youtubePlayer.seekTo(seekTime, true);\r\n    },\r\n    adjustVolume(event) {\r\n      const rect = event.target.getBoundingClientRect();\r\n      const x = event.clientX - rect.left;\r\n      const percentage = (x / rect.width) * 100;\r\n      this.volume = Math.min(Math.max(percentage, 0), 100);\r\n      this.$refs.audioPlayer.volume = this.volume / 100;\r\n    },\r\n    toggleVolume() {\r\n      