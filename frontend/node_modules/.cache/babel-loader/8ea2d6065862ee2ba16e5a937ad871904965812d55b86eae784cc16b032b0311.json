{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport CartInfo from './CartInfo.vue';\nimport { EventBus } from \"../event-bus\";\nexport default {\n  name: 'MainMenu',\n  components: {\n    CartInfo\n  },\n  data() {\n    return {\n      memName: null,\n      backendMessage: null,\n      searchTerm: '' // Added data property for search term\n    };\n  },\n  mounted() {\n    this.memName = sessionStorage.getItem('memName');\n    EventBus.on('loginok', () => {\n      this.memName = sessionStorage.getItem('memName');\n    });\n  },\n  beforeUnmount() {\n    EventBus.off('loginok');\n  },\n  methods: {\n    async memlogout() {\n      EventBus.emit('memlogout');\n      const cf = window.confirm('ต้องการออกจากระบบ?');\n      if (cf) {\n        try {\n          const response = await axios.get(`http://localhost:3000/users/logout`);\n          this.backendMessage = response.data.messagelogout;\n          if (this.backendMessage == 'success') {\n            sessionStorage.clear();\n            this.memName = null;\n            this.$router.push('/');\n          }\n        } catch (err) {\n          console.log(err);\n        }\n      }\n    },\n    searchMusic() {\n      if (this.searchTerm.trim()) {\n        this.$router.push(`/pagemusic/${this.searchTerm.trim()}`);\n      } else {\n        alert(\"Please enter a search term.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","CartInfo","EventBus","name","components","data","memName","backendMessage","searchTerm","mounted","sessionStorage","getItem","on","beforeUnmount","off","methods","memlogout","emit","cf","window","confirm","response","get","messagelogout","clear","$router","push","err","console","log","searchMusic","trim","alert"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/MainMenu.vue"],"sourcesContent":["<template>\r\n  <nav class=\"navbar navbar-expand-lg custom-navbar\">\r\n    <div class=\"container-fluid\">\r\n      <a class=\