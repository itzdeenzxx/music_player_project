{"ast":null,"code":"import \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nexport default {\n  name: 'EnhancedMusicPlayer',\n  data() {\n    return {\n      musics: [],\n      currentPlayingIndex: -1,\n      isPlaying: false,\n      currentTime: 0,\n      duration: 0,\n      volume: 80,\n      isVideoHidden: false,\n      currentView: 'list',\n      isShuffleOn: false,\n      isRepeatOn: false,\n      player: null,\n      views: [{\n        name: 'list',\n        label: 'List View'\n      }, {\n        name: 'cards',\n        label: 'Card View'\n      }, {\n        name: 'albums',\n        label: 'Albums'\n      }, {\n        name: 'popular',\n        label: 'Popular'\n      }]\n    };\n  },\n  computed: {\n    currentMusic() {\n      return this.currentPlayingIndex >= 0 ? this.musics[this.currentPlayingIndex] : null;\n    },\n    progress() {\n      return this.currentTime / this.duration * 100 || 0;\n    },\n    uniqueAlbums() {\n      return [...new Set(this.musics.map(music => music.ms_album))];\n    },\n    randomPopular() {\n      return [...this.musics].sort(() => 0.5 - Math.random()).slice(0, 10);\n    }\n  },\n  methods: {\n    async fetchMusics() {\n      try {\n        const response = await fetch('http://localhost:3000/musics');\n        if (!response.ok) throw new Error('Failed to fetch music data');\n        this.musics = await response.json();\n      } catch (error) {\n        console.error('Error fetching music:', error);\n      }\n    },\n    loadYouTubeAPI() {\n      return new Promise(resolve => {\n        const tag = document.createElement('script');\n        tag.src = 'https://www.youtube.com/iframe_api';\n        const firstScriptTag = document.getElementsByTagName('script')[0];\n        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);\n        window.onYouTubeIframeAPIReady = () => {\n          resolve();\n        };\n      });\n    },\n    async initYouTubePlayer() {\n      if (!window.YT) {\n        await this.loadYouTubeAPI();\n      }\n      this.player = new window.YT.Player('youtube-player', {\n        height: '200',\n        width: '100%',\n        playerVars: {\n          controls: 0,\n          disablekb: 1,\n          enablejsapi: 1,\n          modestbranding: 1,\n          rel: 0\n        },\n        events: {\n          onReady: this.onPlayerReady,\n          onError: event => {\n            console.error('YouTube Player Error:', event);\n          }\n        }\n      });\n    },\n    getYouTubeIdFromUrl(url) {\n      const regExp = /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/;\n      const match = url.match(regExp);\n      return match && match[7].length === 11 ? match[7] : false;\n    },\n    async playMusic(index) {\n      if (this.currentPlayingIndex === index) {\n        this.togglePlay();\n        return;\n      }\n      this.currentPlayingIndex = index;\n      const videoId = this.getYouTubeIdFromUrl(this.musics[index].ms_path);\n      if (!videoId) {\n        console.error('Invalid YouTube URL');\n        return;\n      }\n\n      // ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Ç‡∏∂‡πâ‡∏ô\n      if (!this.player) {\n        await this.initYouTubePlayer();\n      }\n      if (this.player && typeof this.player.loadVideoById === 'function') {\n        this.player.loadVideoById(videoId);\n        this.isPlaying = true;\n      } else {\n        console.error('Player not ready or loadVideoById is not a function');\n      }\n    },\n    togglePlay() {\n      if (!this.player) return;\n      if (this.isPlaying) {\n        this.player.pauseVideo();\n      } else {\n        this.player.playVideo();\n      }\n    },\n    nextTrack() {\n      if (this.currentPlayingIndex < this.musics.length - 1) {\n        this.playMusic(this.currentPlayingIndex + 1);\n      }\n    },\n    previousTrack() {\n      if (this.currentPlayingIndex > 0) {\n        this.playMusic(this.currentPlayingIndex - 1);\n      }\n    },\n    onPlayerStateChange(event) {\n      this.isPlaying = event.data === this.playerStates.PLAYING;\n      if (event.data === this.playerStates.ENDED) {\n        this.nextTrack();\n      }\n    },\n    onPlayerReady() {\n      if (this.player) {\n        this.player.setVolume(this.volume);\n      }\n    },\n    formatTime(seconds) {\n      const minutes = Math.floor(seconds / 60);\n      const remainingSeconds = Math.floor(seconds % 60);\n      return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;\n    },\n    seekTime(event) {\n      const totalWidth = event.currentTarget.offsetWidth;\n      const clickX = event.offsetX;\n      const newTime = clickX / totalWidth * this.duration;\n      this.player.seekTo(newTime);\n    },\n    toggleVolume() {\n      this.volume = this.volume === 0 ? 80 : 0;\n      if (this.player) {\n        this.player.setVolume(this.volume);\n      }\n    },\n    adjustVolume(event) {\n      const totalWidth = event.currentTarget.offsetWidth;\n      const clickX = event.offsetX;\n      const newVolume = clickX / totalWidth * 100;\n      this.volume = Math.min(Math.max(newVolume, 0), 100);\n      if (this.player) {\n        this.player.setVolume(this.volume);\n      }\n    },\n    toggleFavorite() {\n      this.isFavorite = !this.isFavorite;\n    },\n    async chkLogin() {\n      await this.chkSession();\n      if (!this.memEmail) {\n        alert(\"‡∏ï‡πâ‡∏≠‡∏á Login ‡πÄ‡∏Ç‡πâ‡∏≤‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡πà‡∏≠‡∏ô\");\n        return;\n      }\n      await this.chkCart();\n      if (!this.cartId) {\n        await this.addCart();\n      }\n      this.addCartDtl();\n    },\n    musicsByAlbum(album) {\n      return this.musics.filter(music => music.ms_album === album);\n    },\n    findMusicIndex(id) {\n      return this.musics.findIndex(music => music.ms_id === id);\n    },\n    toggleVideoVisibility() {\n      this.isVideoHidden = !this.isVideoHidden;\n    },\n    toggleShuffle() {\n      this.isShuffleOn = !this.isShuffleOn;\n    },\n    toggleRepeat() {\n      this.isRepeatOn = !this.isRepeatOn;\n    },\n    handleTrackEnd() {\n      if (this.isRepeatOn) {\n        this.player.seekTo(0);\n        this.player.playVideo();\n      } else if (this.isShuffleOn) {\n        const nextIndex = Math.floor(Math.random() * this.musics.length);\n        this.playMusic(nextIndex);\n      } else {\n        this.nextTrack();\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","musics","currentPlayingIndex","isPlaying","currentTime","duration","volume","isVideoHidden","currentView","isShuffleOn","isRepeatOn","player","views","label","computed","currentMusic","progress","uniqueAlbums","Set","map","music","ms_album","randomPopular","sort","Math","random","slice","methods","fetchMusics","response","fetch","ok","Error","json","error","console","loadYouTubeAPI","Promise","resolve","tag","document","createElement","src","firstScriptTag","getElementsByTagName","parentNode","insertBefore","window","onYouTubeIframeAPIReady","initYouTubePlayer","YT","Player","height","width","playerVars","controls","disablekb","enablejsapi","modestbranding","rel","events","onReady","onPlayerReady","onError","event","getYouTubeIdFromUrl","url","regExp","match","length","playMusic","index","togglePlay","videoId","ms_path","loadVideoById","pauseVideo","playVideo","nextTrack","previousTrack","onPlayerStateChange","playerStates","PLAYING","ENDED","setVolume","formatTime","seconds","minutes","floor","remainingSeconds","toString","padStart","seekTime","totalWidth","currentTarget","offsetWidth","clickX","offsetX","newTime","seekTo","toggleVolume","adjustVolume","newVolume","min","max","toggleFavorite","isFavorite","chkLogin","chkSession","memEmail","alert","chkCart","cartId","addCart","addCartDtl","musicsByAlbum","album","filter","findMusicIndex","id","findIndex","ms_id","toggleVideoVisibility","toggleShuffle","toggleRepeat","handleTrackEnd","nextIndex"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/TheHome.vue"],"sourcesContent":["<template>\r\n  <div class=\"music-app\">\r\n    <!-- Sidebar Player -->\r\n    <div class=\"player-sidebar\" :class=\"{ 'video-hidden': isVideoHidden }\">\r\n      <div class=\"player-header\">\r\n        <button @click=\"toggleVideoVisibility\" class=\"icon-button\">\r\n          {{ isVideoHidden ? 'üéµ' : 'üé¨' }}\r\n        </button>\r\n        <h3>Now Playing</h3>\r\n      </div>\r\n      \r\n      <div class=\"youtube-container\" v-if=\"!isVideoHidden && currentMusic\">\r\n        <div id=\"youtube-player\"></div>\r\n      </div>\r\n      \r\n      <div class=\"player-controls\">\r\n        <div class=\"song-info\">\r\n          <h4>{{ currentMusic?.ms_name || 'Select a song' }}</h4>\r\n          <p>{{ currentMusic?.ms_album || 'No album' }}</p>\r\n        </div>\r\n        \r\n        <div class=\"progress-bar\">\r\n          <span class=\"time\">{{ formatTime(currentTime) }}</span>\r\n          <div class=\"progress\" @click=\"seekTime\">\r\n            <div class=\"progress-filled\" :style=\"{ width: progress + '%' }\"></div>\r\n          </div>\r\n          <span class=\"time\">{{ formatTime(duration) }}</span>\r\n        </div>\r\n        \r\n        <div class=\"control-buttons\">\r\n          <button @click=\"toggleShuffle\" :class=\"{ active: isShuffleOn }\">üîÄ</button>\r\n          <button @click=\"previousTrack\">‚èÆÔ∏è</button>\r\n          <button @click=\"togglePlay\" class=\"play-button\">\r\n            {{ isPlaying ? '‚è∏Ô∏è' : '‚ñ∂Ô∏è' }}\r\n          </button>\r\n          <button @click=\"nextTrack\">‚è≠Ô∏è</button>\r\n          <button @click=\"toggleRepeat\" :class=\"{ active: isRepeatOn }\">üîÅ</button>\r\n        </div>\r\n        \r\n        <div class=\"volume-control\">\r\n          <button @click=\"toggleVolume\">\r\n            {{ volume === 0 ? 'üîá' : volume < 50 ? 'üîâ' : 'üîä' }}\r\n          </button>\r\n          <div class=\"volume-slider\" @click=\"adjustVolume\">\r\n            <div class=\"volume-filled\" :style=\"{ width: volume + '%' }\"></div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Main Content -->\r\n    <div class=\"main-content\">\r\n      <nav class=\"view-controls\">\r\n        <button \r\n          v-for=\"view in views\" \r\n          :key=\"view.name\"\r\n          @click=\"currentView = view.name\"\r\n          :class=\"{ active: currentView === view.name }\"\r\n        >\r\n          {{ view.label }}\r\n        </button>\r\n      </nav>\r\n\r\n      <!-- List View -->\r\n      <div v-if=\"currentView === 'list'\" class=\"music-list\">\r\n        <div \r\n          v-for=\"(music, index) in musics\" \r\n          :key=\"music.ms_id\"\r\n          class=\"music-item\"\r\n          :class=\"{ playing: currentPlayingIndex === index }\"\r\n          @click=\"playMusic(index)\"\r\n        >\r\n          <div class=\"music-info\">\r\n            <h3>{{ music.ms_name }}</h3>\r\n            <p>{{ music.ms_album }}</p>\r\n            <span class=\"duration\">{{ formatTime(music.ms_duration) }}</span>\r\n          </div>\r\n          <div class=\"music-type\">{{ music.ms_type }}</div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Card View -->\r\n      <div v-if=\"currentView === 'cards'\" class=\"music-cards\">\r\n        <div \r\n          v-for=\"(music, index) in musics\" \r\n          :key=\"music.ms_id\"\r\n          class=\"music-card\"\r\n          @click=\"playMusic(index)\"\r\n        >\r\n          <div class=\"card-image\">\r\n            <div class=\"play-overlay\">\r\n              <span>{{ isPlaying && currentPlayingIndex === index ? '‚è∏Ô∏è' : '‚ñ∂Ô∏è' }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"card-info\">\r\n            <h3>{{ music.ms_name }}</h3>\r\n            <p>{{ music.ms_album }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Album View -->\r\n      <div v-if=\"currentView === 'albums'\" class=\"album-grid\">\r\n        <div \r\n          v-for=\"album in uniqueAlbums\" \r\n          :key=\"album\"\r\n          class=\"album-card\"\r\n        >\r\n          <h2>{{ album }}</h2>\r\n          <div class=\"album-songs\">\r\n            <div \r\n              v-for=\"music in musicsByAlbum(album)\"\r\n              :key=\"music.ms_id\"\r\n              class=\"album-song\"\r\n              @click=\"playMusic(findMusicIndex(music.ms_id))\"\r\n            >\r\n              <span>{{ music.ms_name }}</span>\r\n              <span>{{ formatTime(music.ms_duration) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Popular View -->\r\n      <div v-if=\"currentView === 'popular'\" class=\"popular-section\">\r\n        <h2>Popular Songs</h2>\r\n        <div class=\"popular-grid\">\r\n          <div \r\n            v-for=\"(music, index) in randomPopular\" \r\n            :key=\"music.ms_id\"\r\n            class=\"popular-card\"\r\n            @click=\"playMusic(findMusicIndex(music.ms_id))\"\r\n          >\r\n            <div class=\"popular-rank\">#{{ index + 1 }}</div>\r\n            <div class=\"popular-info\">\r\n              <h3>{{ music.ms_name }}</h3>\r\n              <p>{{ music.ms_album }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'EnhancedMusicPlayer',\r\n  data() {\r\n    return {\r\n      musics: [],\r\n      currentPlayingIndex: -1,\r\n      isPlaying: false,\r\n      currentTime: 0,\r\n      duration: 0,\r\n      volume: 80,\r\n      isVideoHidden: false,\r\n      currentView: 'list',\r\n      isShuffleOn: false,\r\n      isRepeatOn: false,\r\n      player: null,\r\n      views: [\r\n        { name: 'list', label: 'List View' },\r\n        { name: 'cards', label: 'Card View' },\r\n        { name: 'albums', label: 'Albums' },\r\n        { name: 'popular', label: 'Popular' }\r\n      ]\r\n    }\r\n  },\r\n  \r\n  computed: {\r\n    currentMusic() {\r\n      return this.currentPlayingIndex >= 0 ? this.musics[this.currentPlayingIndex] : null\r\n    },\r\n    progress() {\r\n      return (this.currentTime / this.duration) * 100 || 0\r\n    },\r\n    uniqueAlbums() {\r\n      return [...new Set(this.musics.map(music => music.ms_album))]\r\n    },\r\n    randomPopular() {\r\n      return [...this.musics]\r\n        .sort(() => 0.5 - Math.random())\r\n        .slice(0, 10)\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async fetchMusics() {\r\n      try {\r\n        const response = await fetch('http://localhost:3000/musics');\r\n        if (!response.ok) throw new Error('Failed to fetch music data');\r\n        this.musics = await response.json();\r\n      } catch (error) {\r\n        console.error('Error fetching music:', error);\r\n      }\r\n    },\r\n    loadYouTubeAPI() {\r\n  return new Promise((resolve) => {\r\n    const tag = document.createElement('script');\r\n    tag.src = 'https://www.youtube.com/iframe_api';\r\n    const firstScriptTag = document.getElementsByTagName('script')[0];\r\n    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);\r\n\r\n    window.onYouTubeIframeAPIReady = () => {\r\n      resolve();\r\n    };\r\n  });\r\n},\r\n\r\nasync initYouTubePlayer() {\r\n  if (!window.YT) {\r\n    await this.loadYouTubeAPI();\r\n  }\r\n  \r\n  this.player = new window.YT.Player('youtube-player', {\r\n    height: '200',\r\n    width: '100%',\r\n    playerVars: {\r\n      controls: 0,\r\n      disablekb: 1,\r\n      enablejsapi: 1,\r\n      modestbranding: 1,\r\n      rel: 0\r\n    },\r\n    events: {\r\n      onReady: this.onPlayerReady,\r\n     