{"ast":null,"code":"import axios from 'axios';\naxios.defaults.withCredentials = true;\nexport default {\n  name: \"PageMember\",\n  data() {\n    return {\n      memEmail: null,\n      memName: null,\n      imageExists: false,\n      fileMessage: null,\n      file: null,\n      imageTimestamp: Date.now()\n    };\n  },\n  async mounted() {\n    await this.chkSession();\n    this.checkImage();\n  },\n  methods: {\n    onFileChange(e) {\n      this.file = e.target.files[0];\n      if (this.file) {\n        if (!this.file.type.match('image.*')) {\n          this.fileMessage = 'กรุณาเลือกไฟล์รูปภาพเท่านั้น';\n          this.file = null;\n          return;\n        }\n      }\n    },\n    async uploadFile() {\n      if (!this.file) {\n        this.fileMessage = \"กรุณาเลือกไฟล์ก่อนอัพโหลด\";\n        return;\n      }\n      const formData = new FormData();\n      formData.append(\"file\", this.file);\n      try {\n        const response = await axios.post(\"http://localhost:3000/users/uploadImg\", formData, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        this.fileMessage = response.data.message;\n        this.checkImage();\n        this.imageTimestamp = Date.now();\n        this.file = null;\n        const fileInput = document.querySelector('input[type=\"file\"]');\n        if (fileInput) fileInput.value = '';\n      } catch (err) {\n        this.fileMessage = \"เกิดข้อผิดพลาดในการอัพโหลด กรุณาลองใหม่อีกครั้ง\";\n      }\n    },\n    async chkSession() {\n      try {\n        const res = await axios.get(`http://localhost:3000/users/getss`);\n        this.memEmail = res.data.email;\n        this.memName = res.data.name;\n      } catch (err) {\n        console.error(err);\n      }\n    },\n    checkImage() {\n      const image = new Image();\n      image.src = `http://localhost:3000/img_mem/${this.memEmail}.jpg`;\n      image.onload = () => {\n        this.imageExists = true;\n      };\n      image.onerror = () => {\n        this.imageExists = false;\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","defaults","withCredentials","name","data","memEmail","memName","imageExists","fileMessage","file","imageTimestamp","Date","now","mounted","chkSession","checkImage","methods","onFileChange","e","target","files","type","match","uploadFile","formData","FormData","append","response","post","headers","message","fileInput","document","querySelector","value","err","res","get","email","console","error","image","Image","src","onload","onerror"],"sources":["/Users/addeen/Desktop/project/frontend/src/components/PageMember.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n      <div class=\"profile-header\">\r\n        <div class=\"profile-image\">\r\n          <div class=\"image-wrapper\">\r\n            <img\r\n              v-if=\"imageExists\"\r\n              :src=\"`http://localhost:3000/img_mem/${memEmail}.jpg?timestamp=${imageTimestamp}`\"\r\n              :alt=\"memEmail\"\r\n              class=\"profile-pic\"\r\n            />\r\n            <div v-else class=\"placeholder\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"placeholder-icon\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 9a2 2 0 012-2h.93a2 2 0 001.664-.89l.812-1.22A2 2 0 0110.07 4h3.86a2 2 0 011.664.89l.812 1.22A2 2 0 0018.07 7H19a2 2 0 012 2v9a2 2 0 01-2 2H5a2 2 0 01-2-2V9z\" />\r\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 13a3 3 0 11-6 0 3 3 0 016 0z\" />\r\n              </svg>\r\n            </div>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"profile-info\">\r\n          <h2 class=\"profile-name\">{{ memName }}</h2>\r\n          <p class=\"profile-email\">{{ memEmail }}</p>\r\n        </div>\r\n      </div>\r\n  \r\n      <form @submit.prevent=\"uploadFile\" class=\"upload-form\">\r\n        <label class=\"upload-label\">อัพโหลดรูปโปรไฟล์</label>\r\n        <div class=\"upload-input-container\">\r\n          <input\r\n            type=\"file\"\r\n            @change=\"onFileChange\"\r\n            required\r\n            accept=\"image/*\"\r\n            class=\"upload-input\"\r\n          />\r\n          <button\r\n            type=\"submit\"\r\n            class=\"upload-button\"\r\n            :disabled=\"!file\"\r\n          >\r\n            อัพโหลด\r\n          </button>\r\n        </div>\r\n  \r\n        <!-- Success/Error Message -->\r\n        <div v-if=\"fileMessage\" class=\"file-message\" :class=\"{ success: fileMessage !== 'fail', error: fileMessage === 'fail' }\">\r\n          {{ fileMessage }}\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  \r\n  <script>\r\nimport axios from 'axios'\r\naxios.defaults.withCredentials = true\r\n\r\nexport default {\r\n  name: \"PageMember\",\r\n  data() {\r\n    return {\r\n      memEmail: null,\r\n      memName: null,\r\n      imageExists: false,\r\n      fileMessage: null,\r\n      file: null,\r\n      imageTimestamp: Date.now(),\r\n    }\r\n  },\r\n  \r\n  async mounted() {\r\n    await this.chkSession()\r\n    this.checkImage()\r\n  },\r\n\r\n  methods: {\r\n    onFileChange(e) {\r\n      this.file = e.target.files[0]\r\n      \r\n      if (this.file) {\r\n        if (!this.file.type.match('image.*')) {\r\n          this.fileMessage = 'กรุณาเลือกไฟล์รูปภาพเท่านั้น'\r\n          this.file = null\r\n          return\r\n        }\r\n      }\r\n    },\r\n\r\n    async uploadFile() {\r\n      if (!this.file) {\r\n        this.fileMessage = \"กรุณาเลือกไฟล์ก่อนอัพโหลด\"\r\n        return\r\n      }\r\n\r\n      const formData = new FormData()\r\n      formData.append(\"file\", this.file)\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          \"http://localhost:3000/users/uploadImg\",\r\n          formData,\r\n          {\r\n            headers: {\r\n              \"Content-Type\": \"multipart/form-data\",\r\n            },\r\n          }\r\n        )\r\n        this.fileMessage = response.data.message\r\n        this.checkImage()\r\n        this.imageTimestamp = Date.now()\r\n        \r\n        this.file = null\r\n        const fileInput = document.querySelector('input[type=\"file\"]')\r\n        if (fileInput) fileInput.value = ''\r\n      } catch (err) {\r\n        this.fileMessage = \"เกิดข้อผิดพลาดในการอัพโหลด กรุณาลองใหม่อีกครั้ง\"\r\n      }\r\n    },\r\n\r\n    async chkSession() {\r\n      try {\r\n        const res = await axios.get(`http://localhost:3000/users/getss`)\r\n        this.memEmail = res.data.email\r\n        this.memName = res.data.name\r\n      } catch (err) {\r\n        console.error(err)\r\n      }\r\n    },\r\n\r\n    checkImage() {\r\n      const image = new Image()\r\n      image.src = `http://localhost:3000/img_mem/${this.memEmail}.jpg`\r\n      image.onload = () => {\r\n        this.imageExists = true\r\n      }\r\n      image.onerror = () => {\r\n        this.imageExists = false\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  min-height: 100vh;\r\n  /* background: linear-gradient(to bottom right, #ebf8ff, #e0e7ff, #f0abfc); */\r\n  padding: 3rem 1rem;\r\n}\r\n\r\n.profile-header {\r\n  background: rgba(255, 255, 255, 0.8);\r\n  backdrop-filter: blur(10px);\r\n  border-radius: 1.5rem;\r\n  padding: 2rem;\r\n  margin-bottom: 2rem;\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 2rem;\r\n}\r\n\r\n.profile-image {\r\n  flex: 1;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.image-wrapper {\r\n  position: relative;\r\n  width: 160px;\r\n  height: 160px;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  background: linear-gradient(to bottom right, #3b82f6, #8b5cf6);\r\n}\r\n\r\n.profile-pic {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.image-wrapper:hover .profile-pic {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.placeholder {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: linear-gradient(to bottom right, #f3f4f6, #e5e7eb);\r\n}\r\n\r\n.placeholder-icon {\r\n  width: 48px;\r\n  height: 48px;\r\n  color: rgba(255, 255, 255, 0.8);\r\n}\r\n\r\n.profile-info {\r\n  flex: 2;\r\n  text-align: center;\r\n}\r\n\r\n.profile-name {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  background: linear-gradient(to right, #3b82f6, #8b5cf6);\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n}\r\n\r\n.profile-email {\r\n  font-size: 1.25rem;\r\n  color: #4b5563;\r\n}\r\n\r\n.upload-form {\r\n  background: rgba(255, 255, 255, 0.8);\r\n  backdrop-filter: blur(10px);\r\n  border-radius: 1.5rem;\r\n  padding: 2rem;\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.upload-label {\r\n  display: block;\r\n  font-size: 1.25rem;\r\n  font-weight: medium;\r\n  color: transparent;\r\n  background: linear-gradient(to right, #3b82f6, #8b5cf6);\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.upload-input-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 1rem;\r\n}\r\n\r\n.upload-input {\r\n  flex-grow: 1;\r\n  padding: 0.75rem;\r\n  border-radius: 9999px;\r\n  border: 2px solid transparent;\r\n  background: linear-gradient(to right, #3b82f6, #8b5cf6);\r\n  color: white;\r\n  cursor: pointer;\r\n}\r\n\r\n.upload-button {\r\n  padding: 0.75rem 1.5rem;\r\n  border-radius: 9999px;\r\n  background: linear-gradient(to right, #3b82f6, #8b5cf6);\r\n  color: white;\r\n  cursor: pointer;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.upload-button:disabled {\r\n  opacity: 0.5;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.file-message {\r\n  margin-top: 1rem;\r\n  padding: 1rem;\r\n  border-radius: 0.5rem;\r\n}\r\n\r\n.file-message.success {\r\n  background: rgba(52, 211, 153, 0.1);\r\n  color: #4ade80;\r\n}\r\n\r\n.file-message.error {\r\n  background: rgba(248, 113, 113, 0.1);\r\n  color: #f87171;\r\n}\r\n</style>\r\n"],"mappings":"AAuDA,OAAOA,KAAI,MAAO,OAAM;AACxBA,KAAK,CAACC,QAAQ,CAACC,eAAc,GAAI,IAAG;AAEpC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE,IAAI;MACbC,WAAW,EAAE,KAAK;MAClBC,WAAW,EAAE,IAAI;MACjBC,IAAI,EAAE,IAAI;MACVC,cAAc,EAAEC,IAAI,CAACC,GAAG,CAAC;IAC3B;EACF,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,UAAU,CAAC;IACtB,IAAI,CAACC,UAAU,CAAC;EAClB,CAAC;EAEDC,OAAO,EAAE;IACPC,YAAYA,CAACC,CAAC,EAAE;MACd,IAAI,CAACT,IAAG,GAAIS,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;MAE5B,IAAI,IAAI,CAACX,IAAI,EAAE;QACb,IAAI,CAAC,IAAI,CAACA,IAAI,CAACY,IAAI,CAACC,KAAK,CAAC,SAAS,CAAC,EAAE;UACpC,IAAI,CAACd,WAAU,GAAI,8BAA6B;UAChD,IAAI,CAACC,IAAG,GAAI,IAAG;UACf;QACF;MACF;IACF,CAAC;IAED,MAAMc,UAAUA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACd,IAAI,EAAE;QACd,IAAI,CAACD,WAAU,GAAI,2BAA0B;QAC7C;MACF;MAEA,MAAMgB,QAAO,GAAI,IAAIC,QAAQ,CAAC;MAC9BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACjB,IAAI;MAEjC,IAAI;QACF,MAAMkB,QAAO,GAAI,MAAM3B,KAAK,CAAC4B,IAAI,CAC/B,uCAAuC,EACvCJ,QAAQ,EACR;UACEK,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CACF;QACA,IAAI,CAACrB,WAAU,GAAImB,QAAQ,CAACvB,IAAI,CAAC0B,OAAM;QACvC,IAAI,CAACf,UAAU,CAAC;QAChB,IAAI,CAACL,cAAa,GAAIC,IAAI,CAACC,GAAG,CAAC;QAE/B,IAAI,CAACH,IAAG,GAAI,IAAG;QACf,MAAMsB,SAAQ,GAAIC,QAAQ,CAACC,aAAa,CAAC,oBAAoB;QAC7D,IAAIF,SAAS,EAAEA,SAAS,CAACG,KAAI,GAAI,EAAC;MACpC,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAAC3B,WAAU,GAAI,iDAAgD;MACrE;IACF,CAAC;IAED,MAAMM,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMsB,GAAE,GAAI,MAAMpC,KAAK,CAACqC,GAAG,CAAC,mCAAmC;QAC/D,IAAI,CAAChC,QAAO,GAAI+B,GAAG,CAAChC,IAAI,CAACkC,KAAI;QAC7B,IAAI,CAAChC,OAAM,GAAI8B,GAAG,CAAChC,IAAI,CAACD,IAAG;MAC7B,EAAE,OAAOgC,GAAG,EAAE;QACZI,OAAO,CAACC,KAAK,CAACL,GAAG;MACnB;IACF,CAAC;IAEDpB,UAAUA,CAAA,EAAG;MACX,MAAM0B,KAAI,GAAI,IAAIC,KAAK,CAAC;MACxBD,KAAK,CAA